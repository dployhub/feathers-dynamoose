{
  "name": "@dployhub/feathers-dynamoose",
  "description": "Feathers service with AWS DynamoDb via dynamoose",
  "version": "0.3.2-alpha.0",
  "homepage": "https://github.com/dployhub/feathers-dynamoose",
  "main": "build/main.js",
  "keywords": [
    "feathers",
    "feathers-plugin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/dployhub/feathers-dynamoose.git"
  },
  "author": {
    "name": "Andrew Chak",
    "email": "andrew@dploy.com.au",
    "url": "https://dployhub.github.io"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/dployhub/feathers-dynamoose/issues"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "build": "backpack build",
    "dev": "backpack",
    "lint": "xo",
    "prepublish": "npm run build",
    "test": "jest && codecov",
    "test:mutation": "jest && codecov && stryker run && rimraf .stryker-tmp"
  },
  "directories": {
    "src": "src"
  },
  "dependencies": {
    "dynamoose": "^1.10.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "backpack-core": "^0.8.4",
    "chance": "^1.0.18",
    "codecov": "^3.5.0",
    "dynamodb-local": "^0.0.24",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "stryker": "^0.34.0",
    "stryker-api": "^0.23.0",
    "stryker-javascript-mutator": "^0.13.0",
    "stryker-jest-runner": "^1.4.1",
    "xo": "^0.23.0"
  },
  "xo": {
    "space": true
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "globalSetup": "<rootDir>/tests/setup-dynamodb.js",
    "globalTeardown": "<rootDir>/tests/teardown-dynamodb.js",
    "globals": {
      "__DEV__": true
    },
    "setupFiles": [
      "<rootDir>/tests/set-dynamodb-config.js"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
